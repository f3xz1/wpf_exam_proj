<Window x:Class="final_proj_wpf.register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:final_proj_wpf"
        mc:Ignorable="d"
        Title="register" Height="330" Width="300">
    <StackPanel Background="Blue">
        <TextBox Margin="10" Height="24" Name="Username_textbox"/>
        <PasswordBox Margin="10" Height="24" Name="Password1_textbox"/>
        <PasswordBox Margin="10" Height="24" Name="Password2_textbox"/>
        <TextBox Margin="10" Height="24" Name="Name_textbox"/>
        <TextBox Margin="10" Height="24" Name="Surname_textbox"/>
        <Button Content="Register" Margin="10" Click="Reg_Button_Click"/>
        <Button Content="Cancel" Margin="10" Click="Cancel_Button_Click"/>
    </StackPanel>
</Window>
